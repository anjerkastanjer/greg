CREATE\040TABLE\040pets\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040user_id\040INT\040NOT\040NULL,
\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,
\040\040\040\040type\040VARCHAR(50)\040NOT\040NULL,\040\040--\040Example:\040dog,\040cat,\040etc.
\040\040\040\040description\040TEXT,
\040\040\040\040photo\040VARCHAR(255),
\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,
\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,
\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id)\040ON\040DELETE\040CASCADE
);
CREATE\040TABLE\040pets\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040user_id\040INT\040NOT\040NULL,\040\040\040\040\040name\040VARCHAR(255)\040NOT\040NULL,\040\040\040\040\040type\040VARCHAR(50)\040NOT\040NULL,\040\040\040\040\040\040\040description\040TEXT,\040\040\040\040\040photo\040VARCHAR(255),\040\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,\040\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,\040\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id)\040ON\040DELETE\040CASCADE\040);
CREATE\040TABLE\040sitting_requests\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040user_id\040INT\040NOT\040NULL,
\040\040\040\040pet_id\040INT\040NOT\040NULL,
\040\040\040\040start_date\040DATE\040NOT\040NULL,
\040\040\040\040end_date\040DATE\040NOT\040NULL,
\040\040\040\040price\040DECIMAL(10,\0402)\040NOT\040NULL,
\040\040\040\040status\040ENUM('open',\040'accepted',\040'completed')\040DEFAULT\040'open',
\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,
\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,
\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id)\040ON\040DELETE\040CASCADE,
\040\040\040\040FOREIGN\040KEY\040(pet_id)\040REFERENCES\040pets(id)\040ON\040DELETE\040CASCADE
);
CREATE\040TABLE\040sitting_requests\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040user_id\040INT\040NOT\040NULL,\040\040\040\040\040pet_id\040INT\040NOT\040NULL,\040\040\040\040\040start_date\040DATE\040NOT\040NULL,\040\040\040\040\040end_date\040DATE\040NOT\040NULL,\040\040\040\040\040price\040DECIMAL(10,\0402)\040NOT\040NULL,\040\040\040\040\040status\040ENUM('open',\040'accepted',\040'completed')\040DEFAULT\040'open',\040\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,\040\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,\040\040\040\040\040FOREIGN\040KEY\040(user_id)\040REFERENCES\040users(id)\040ON\040DELETE\040CASCADE,\040\040\040\040\040FOREIGN\040KEY\040(pet_id)\040REFERENCES\040pets(id)\040ON\040DELETE\040CASCADE\040);
CREATE\040TABLE\040reviews\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040request_id\040INT\040NOT\040NULL,
\040\040\040\040reviewer_id\040INT\040NOT\040NULL,\040\040--\040The\040user\040who\040leaves\040the\040review
\040\040\040\040rating\040INT\040CHECK\040(rating\040>=\0401\040AND\040rating\040<=\0405),
\040\040\040\040comment\040TEXT,
\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,
\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,
\040\040\040\040FOREIGN\040KEY\040(request_id)\040REFERENCES\040sitting_requests(id)\040ON\040DELETE\040CASCADE,
\040\040\040\040FOREIGN\040KEY\040(reviewer_id)\040REFERENCES\040users(id)\040ON\040DELETE\040CASCADE
);
CREATE\040TABLE\040reviews\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040request_id\040INT\040NOT\040NULL,\040\040\040\040\040reviewer_id\040INT\040NOT\040NULL,\040\040\040\040\040\040\040rating\040INT\040CHECK\040(rating\040>=\0401\040AND\040rating\040<=\0405),\040\040\040\040\040comment\040TEXT,\040\040\040\040\040created_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP,\040\040\040\040\040updated_at\040TIMESTAMP\040DEFAULT\040CURRENT_TIMESTAMP\040ON\040UPDATE\040CURRENT_TIMESTAMP,\040\040\040\040\040FOREIGN\040KEY\040(request_id)\040REFERENCES\040sitting_requests(id)\040ON\040DELETE\040CASCADE,\040\040\040\040\040FOREIGN\040KEY\040(reviewer_id)\040REFERENCES\040users(id)\040ON\040DELETE\040CASCADE\040);
